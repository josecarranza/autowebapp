<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="en-US" id="com.alertux.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <icon density="ldpi" src="www/res/icons/android/icon-36-ldpi.png" />
    <icon density="mdpi" src="www/res/icons/android/icon-48-mdpi.png" />
    <icon density="hdpi" src="www/res/icons/android/icon-72-hdpi.png" />
    <icon density="xhdpi" src="www/res/icons/android/icon-96-xhdpi.png" />
    <splash density="land-hdpi" src="www/res/screens/android/screen-hdpi-landscape.png" />
    <splash density="land-ldpi" src="www/res/screens/android/screen-ldpi-landscape.png" />
    <splash density="land-mdpi" src="www/res/screens/android/screen-mdpi-landscape.png" />
    <splash density="land-xhdpi" src="www/res/screens/android/screen-xhdpi-landscape.png" />
    <splash density="port-hdpi" src="www/res/screens/android/screen-hdpi-portrait.png" />
    <splash density="port-ldpi" src="www/res/screens/android/screen-ldpi-portrait.png" />
    <splash density="port-mdpi" src="www/res/screens/android/screen-mdpi-portrait.png" />
    <splash density="port-xhdpi" src="www/res/screens/android/screen-xhdpi-portrait.png" />
    <name>AMARSAL - APP</name>
    <description> Aplicación Cámara de Comercio e Industria</description>
    <author email="info@web-informatica.com" href="http://www.web-informatica.com">
        Web Informatica
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="SplashScreenDelay" value="10000" />
    <preference name="FadeSplashScreen" value="true" />
    <preference name="FadeSplashScreenDuration" value="1.0" />
    <preference name="windows-target-version" value="8.1" />
    <preference name="windows-phone-target-version" value="8.1" />
    <vs:plugin name="org.apache.cordova.camera" version="0.3.6" />
    <vs:plugin name="org.apache.cordova.geolocation" version="0.3.12" />
    <vs:plugin name="org.apache.cordova.globalization" version="0.3.4" />
    <vs:plugin name="org.apache.cordova.dialogs" version="0.3.0" />
    <vs:plugin name="org.apache.cordova.statusbar" version="0.1.10" />
    <vs:plugin name="org.apache.cordova.vibration" version="0.3.13" />
    <vs:plugin name="org.apache.cordova.media-capture" version="0.3.6" />
    <vs:plugin name="org.apache.cordova.device-orientation" version="0.3.11" />
    <vs:plugin name="org.apache.cordova.file" version="1.3.3" />
    <vs:plugin name="org.apache.cordova.battery-status" version="0.2.12" />
    <vs:plugin name="org.apache.cordova.splashscreen" version="1.0.0" />
    <preference name="KeepRunning" value="True" />
    <preference name="ShowTitle" value="True" />
    <preference name="InAppBrowserStorageEnabled" value="True" />
    <preference name="SuppressesIncrementalRendering" value="True" />
    <vs:plugin name="org.apache.cordova.device" version="0.3.0" />
    <vs:plugin name="org.apache.cordova.network-information" version="0.2.15" />
    <preference name="android-minSdkVersion" value="9" />
    <preference name="android-maxSdkVersion" value="23" />
    <preference name="android-targetSdkVersion" value="23" />
    <preference name="AndroidLaunchMode" value="standard" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
</widget>
