<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="en-US" id="com.autoweb.app" version="1.1.6" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <preference name="loglevel" value="DEBUG" />
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>AUTOWEBAPP</name>
    <description>AutoWeb APP</description>
    <author email="info@web-informatica.com" href="http://www.web-informatica.com">Web Informatica</author>
    <content src="index.html" />
