    <vs:plugin name="org.apache.cordova.geolocation" />
    <vs:plugin name="org.apache.cordova.globalization" />
    <vs:plugin name="org.apache.cordova.dialogs" />
    <vs:plugin name="org.apache.cordova.statusbar" />
    <vs:plugin name="org.apache.cordova.vibration" />
    <vs:plugin name="org.apache.cordova.media-capture" />
    <vs:plugin name="org.apache.cordova.device-orientation" />
    <vs:plugin name="org.apache.cordova.file" />
    <vs:plugin name="org.apache.cordova.battery-status" />
    <vs:plugin name="org.apache.cordova.splashscreen" />
